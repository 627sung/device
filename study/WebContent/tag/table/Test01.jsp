<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>table 배워봅시다.</title>
</head>
<body>
	<table border="1">
		<thead>
			<!-- 테이블헤더(제목) -->
			<tr>
				<!--  줄 (table row) -->
				<th>번호</th>
				<th>아이디</th>
				<th>닉네임</th>
				<th>가입일</th>
			</tr>
		<tbody>
			<!--테이블바디(내용)  줄먼저 만들고 그 안에 칸을 만들자  -->
			<tr>
				<td>1</td>
				<td>test</td>
				<td>테스트</td>
				<td>2020-06-05</td>

			</tr>
		</tbody>
	</table>
</body>
</html>